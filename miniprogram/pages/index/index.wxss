/**index.wxss**/

page {
    padding-top: 110rpx;
}

.search_input {
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
}

.index_swiper swiper {
    width: 750rpx;
    height: 340rpx;
}

.index_swiper swiper swiper-item image {
    width: 100%;
}

.index_cate {
    display: flex;
}

.index_cate navigator {
    flex: 1;
    padding: 20rpx;
}

.index_cate navigator image {
    width: 100%;
}

.floor_group .floor_title {
    padding: 10rpx 0;
}

.floor_group .floor_title image {
    width: 100%;
}

.floor_group .floor_list {
    overflow: hidden;
    margin: 0 10rpx;
}

.floor_group .floor_list navigator {
    width: 33.33%;
    float: left;
}

.floor_group .floor_list navigator:nth-last-child(-n+4) {
    height: 27.727vw;
    border-left: 10rpx solid #fff;
    border-bottom: 10rpx solid #fff;
    box-sizing: border-box;
}

.floor_group .floor_list navigator image {
    width: 100%;
    height: 100%;
}